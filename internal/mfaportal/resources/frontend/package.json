    "@fortawesome/vue-fontawesome": "^3.0.8",
    "axios": "^1.7.7",
    "pinia": "^2.2.2",
    "vue": "^3.5.4",
    "vue-router": "^4.4.4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^18.19.50",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^7.1.0",
