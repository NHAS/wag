    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "vue-tsc": "^2.1.6"
